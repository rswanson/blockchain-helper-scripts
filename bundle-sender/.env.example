# Flashbots Bundle Sender Configuration
# Copy this file to .env and fill in your values

# Builder and RPC URLs
BUILDER_URL=https://host-builder-rpc.parmigiana.signet.sh
RPC_URL=https://host-rpc.parmigiana.signet.sh

# Transaction value (default: 1gwei)
VALUE=1gwei

# Priority fee in wei (default: 2 gwei = 2000000000)
PRIORITY_FEE=2000000000

# AWS Configuration for signing
AWS_DEFAULT_REGION=us-east-1
AWS_PROFILE=init4-dev-poweruser
AWS_KMS_KEY_ID=arn:aws:kms:us-east-1:637423570300:alias/pecorino-poa-bank

# Temporary private key for transaction creation
# This key is used to create and sign test transactions
# WARNING: Do not commit this file with real private keys!
TEMP_PRIVATE_KEY=

# Optional: Pre-signed transaction (if you want to skip transaction generation)
# SIGNED_TX=0x...
